    "css-loader": "^6.4.0",
    "fs-extra": "^10.0.0",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss-loader": "^6.2.0",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
